{
  "name": "@haleyjs/vue",
  "version": "0.0.1",
  "author": "TheHaleyProject",
  "license": "MIT",
  "description": "Contains some useful components for Vue JS",
  "main": "dist/haley-vue.umd.cjs",
  "module": "dist/haley-vue.js",
  "typings": "dist/index.d.ts",
  "type": "module",
  "files": [
    "dist",
    "src",
    "volar.d.ts",
    "*.d.ts"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/haley-vue.js",
      "require": "./dist/haley-vue.umd.cjs"
    },
    "./*":"./*",
    "./dist/style.css": "./dist/style.css",
    "./volar": {
      "types": "./volar.d.ts"
    }
  },
  "keywords": [
    "vue3",
    "haley vue",
    "haley",
    "haley js"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/TheHaleyProject/HaleyVue"
  },
  "scripts": {
    "build": "vite build",
    "dev": "vite",
    "preview": "vite preview"
  },
  "peerDependencies": {
    "vue": "^3.2.47"
  },
  "devDependencies": {
    "@types/node": "^18.19.26",
    "@vitejs/plugin-vue": "^3.2.0",
    "autoprefixer": "^10.4.19",
    "postcss": "^8.4.21",
    "prettier": "^2.8.8",
    "prettier-plugin-tailwindcss": "^0.2.8",
    "tailwindcss": "^3.4.1",
    "typescript": "^4.9.5",
    "unplugin-auto-import": "^0.12.2",
    "vite": "^3.2.8",
    "vite-plugin-dts": "^3.8.3",
    "vite-tsconfig-paths": "^4.3.2",
    "vue": "^3.2.47",
    "vue-tsc": "^1.8.27"
  }
}
